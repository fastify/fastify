{
  "problemMatcher": [
    {
      "owner": "markdownlint",
      "pattern": [
        {
          "regexp": "^Error: (\\w+\\\/?\\w+(.md)?):(\\d+):(\\d+) (MD\\d+)\\/(.+)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "code": 4,
          "message": 5
        }
      ]
    }
  ]
}
